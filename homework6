#Создание переменной и присваивание значения словаря, вывод
my_dict = {'Max': 1995, 'Alex': 2005, 'Sahsa': 2000}
print(my_dict)
#Вывод значения по существующему и несуществующему ключу
a = my_dict['Max']
print(a)

print(my_dict.get('Lena', 'Такого ключа нет'))
#Добавление пар того же формата
my_dict.update({'Kirill': 2003,
                'Ira' : 1997})
print(my_dict)

#Удаление одной из пар по ключу и вывод значения
b = my_dict.pop('Alex')
print(b)
#Вывод словаря
print(my_dict)

#Создание множества
my_set = {1,2,3,4,1,2,3}
print(my_set)
#Добавить 2 элемента во множество
my_set.add(6)
my_set.add(8)
print(my_set)
#Удалить любой элемент из множества
my_set.discard(1)
print(my_set)
